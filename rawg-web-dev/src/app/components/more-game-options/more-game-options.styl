.more-game-options
  $font14-small()
  min-width 256px
  color black
  background-color white
  border-radius 6px
  overflow hidden

.more-game-options__bottom
  padding 4px 16px
  background-color rgba(#151515, 0.05)

.more-game-options__icon
  width 14px
  height 14px
  margin-right 6px

  svg
    width 100%
    height 100%

  path
    fill black
