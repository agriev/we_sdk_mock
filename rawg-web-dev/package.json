{
  "name": "rawg",
  "version": "1.0.0",
  "description": "RAWG",
  "scripts": {
    "start:dev": "yarn && yarn build:dev && yarn server:dev",
    "start:prod": "yarn && yarn build:prod && yarn server:prod",
    "start:api-prod": "cross-env API_ADDRESS_RU=https://ag.ru API_ADDRESS_EN=https://api.rawg.io SERVER_API_ADDRESS=https://api.rawg.io yarn && yarn build:prod && yarn server:prod",
    "start:dev-api-prod": "cross-env API_ADDRESS_RU=https://ag.ru API_ADDRESS_EN=https://api.rawg.io SERVER_API_ADDRESS=https://api.rawg.io yarn && yarn build:dev && yarn server:dev",
    "prebuild:all": "mkdirp ./build && rimraf ./build/* && ncp ./src/ssr/client/assets ./build",
    "build:dev": "yarn prebuild:all",
    "build:prod": "yarn prebuild:all && cross-env NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=4096\" babel-node ./webpack/webpack.prod-builder.js",
    "server:dev": "cross-env NODE_ENV=development NODE_OPTIONS=\"--max-old-space-size=4096\" babel-node ./webpack/webpack.dev-runner.js",
    "server:prod": "cross-env NODE_ENV=production NODE_OPTIONS=\"--max-old-space-size=4096\" node ./build/server.js",
    "test": "test",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "plop --plopfile generators/index.js",
    "generate:container": "plop --plopfile generators/index.js container",
    "generate:component": "plop --plopfile generators/index.js component",
    "generate:route": "plop --plopfile generators/index.js route",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/behindthegames/rawg-web.git"
